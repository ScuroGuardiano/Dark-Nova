    "@types/colors": "^1.2.1",
    "@types/convict": "^4.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/ejs": "^2.6.1",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.120",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/sequelize": "^4.28.1",
    "@types/winston": "^2.4.4",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
